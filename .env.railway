# Laravel Transactix - Railway Environment Configuration
# Copy these variables to your Railway project environment

# Application
APP_NAME=Transactix
APP_ENV=production
APP_DEBUG=false
APP_URL=https://your-app-name.up.railway.app
APP_KEY=

# Database (Supabase)
DB_CONNECTION=pgsql
DB_HOST=
DB_PORT=5432
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=
DB_SSLMODE=require

# Supabase API credentials
SUPABASE_URL=
SUPABASE_KEY=
SUPABASE_SECRET=

# Laravel optimizations for Railway
LOG_CHANNEL=stderr
LOG_LEVEL=error
SESSION_DRIVER=file
CACHE_STORE=file
QUEUE_CONNECTION=sync
FILESYSTEM_DISK=local

# Security
BCRYPT_ROUNDS=12

# Railway-specific
PORT=80
